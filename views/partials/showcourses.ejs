<% for(var i=0;i<data.length;i++){ %>
    <tr>
    <td><%=i+1%></td>
    <td><%=data[i].coursename%></td>
    <td><%=data[i].coursecode%></td>
    <td><%=data[i].coursecategory%></td>
    <td><%=data[i].status%></td>
   
    <td>
        <a href='/edit_course/<%=data[i]._id%>/<%=data[i].coursename%>' class="btn border-shadow update">
        <span class="text-gradient"><button type="button" class="btn btn-primary">EDIT</button></span>
        </a>
    
        <a href='/delete_course/<%=data[i]._id%>/<%=data[i].coursename%>' class="btn border-shadow update">
        <span class="text-gradient"><button type="button" class="btn btn-danger">DELETE</button></span>
        </a>
    </td>

    </tr>
<%}%>