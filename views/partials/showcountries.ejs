<% for(var i=0;i<data.length;i++){ %>
    <tr>
    <td><%=i+1%></td>
    <td><%=data[i].country_name%></td>
    <td><%=data[i].country_code%></td>
    <td><%=data[i].currency%></td>
    <td><%=data[i].number%></td>
    <td><%=data[i].status%></td>
    
    <td>
        <a href='/edit_countries/<%=data[i]._id%>/<%=data[i].country_name%>' class="btn border-shadow update">
        <span class="text-gradient"><button type="button" class="btn btn-primary">EDIT</button></span>
        </a>
    
        <a href='/delete_countries/<%=data[i]._id%>/<%=data[i].country_name%>' class="btn border-shadow update">
        <span class="text-gradient"><button type="button" class="btn btn-danger">DELETE</button></span>
        </a>
    </td>

    </tr>
<%}%>